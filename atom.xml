---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Nicole Harris</title>
    <link href="http://whoisnicoleharris.com/atom.xml" rel="self"/>
    <link href="http://whoisnicoleharris.com/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://whoisnicoleharris.com/</id>
    <author>
        <name>Nicole Harris</name>
        <email>n.harris@kabucreative.com.au</email>
    </author>
    {% for post in site.posts %}
        <entry>
            <title>{{ post.title }}</title>
            <link href="{{ site.url }}{{ post.url }}"/>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <id>{{ site.url }}{{ post.id }}</id>
            <content type="html">{{ post.content | xml_escape }}</content>
        </entry>
    {% endfor %}
</feed>
